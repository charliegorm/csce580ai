# Model Artifacts (TOO LARGE)
*.bin
*.safetensors
*.onnx
*.h5

# Data Folders (LARGE)
ProjectB/data/raw/
ProjectB/data/interim/
ProjectB/data/processed/
!ProjectB/data/raw/.gitkeep
!ProjectB/data/interim/.gitkeep
!ProjectB/data/processed/.gitkeep

# Large HuggingFace cache files
*.arrow
*.bin
*.safetensors

# HuggingFace cache directories
# (prevents accidentally tracking ~/.cache/huggingface or local cache clones)
**/huggingface/
**/.cache/huggingface/
**/.cache/

# blocking distilbert model artifacts
ProjectB/models/distilbert/
!ProjectB/models/distilbert/.gitkeep

# blocking training runs/checkpoints
**/runs/
**/checkpoint-*/

# preventing auto-generated trainer logs + state
**/trainer_state.json
**/trainer_config.json
**/optimizer.pt
**/scheduler.pt
**/training_args.bin

# Torch large files
*.pt
*.pth

# making sure nothing under models gets committed 
ProjectB/models/**/*.pt
ProjectB/models/**/*.pth
ProjectB/models/**/checkpoint-*/
ProjectB/models/**/optimizer.pt
ProjectB/models/**/trainer_state.json

# MacOS / VSCode junk
.DS_Store
.vscode/
